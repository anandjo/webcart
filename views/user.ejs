<html>
<head>
  <title><%=title%></title>
  <link rel="stylesheet" type="text/css" href="/stylesheets/style.css"></link> 
  <link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap.css"></link> 
  <link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap-theme.css"></link>  
</head>
<body>
  <div class="container ">
  <form name="frmUser" method="POST" action="/submit"> 
    <div class="row">
        <div class="col-md-2"></div>
        <div class="col-md-8">
          <div class="form-group">
            <label>User Name</label>
            <div class="">
                <input type="text" class="form-control" name="empno" id="empno" />
            </div>
          </div>

        </div>
        <div class="col-md-2"></div>
    </div>
     <div class="row">
        <div class="col-md-2"></div>
        <div class="col-md-8">
          <div class="form-group">
            <label>Address</label>
            <div class="">
                <input type="text" class="form-control" name="address" id="address" />
            </div>
          </div>

        </div>
        <div class="col-md-2"></div>
    </div>

     <div class="row">
        <div class="col-md-2"></div>
        <div class="col-md-8">
          <div class="form-group">
            
            <div class="">
                 <button type = "submit" class = "btn btn-default">Submit</button>
            </div>
          </div>

        </div>
        <div class="col-md-2"></div>
    </div>
    </form>


    
  </div>

  <div class="container">

    
    <table class="table table-striped">
      <tr>
       <th>Emp No</th>
       <th>Emp Name</th>
        <th>Address</th>
     </tr>
    <% if (user.length) { %>
       <% user.forEach(function(users){ %>
       <tr>
        <td><%= users['empno'] %></td>
        <td><%= users['empname'] %></td>
        <td><%= users['address'] %></td>
      </tr>
      <% }) %>
 
  <% } %>
 </table>
</div>
<body>
</html>